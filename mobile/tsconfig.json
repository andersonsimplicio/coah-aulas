{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "jsx": "react-native",
    "strict": true,
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "*": ["*", "src/*"]
    }
  },
  "exclude": ["node_modules"]
}

  